{
    "name": "smichaelsen/autourls",
    "description": "Zero configuration speaking URLs for TYPO3",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0+",
    "require": {
        "typo3/cms": "7.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.6.*",
        "phpunit/php-code-coverage": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "Smichaelsen\\Autourls\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smichaelsen\\Autourls\\Tests\\": "Tests/",
            "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/",
            "TYPO3\\CMS\\Fluid\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/fluid/Tests/"
        }
    },
    "replace": {
        "autourls": "self.version",
        "typo3-ter/autourls": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "preferred-install": {
            "typo3/cms": "source"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "mkdir -p .Build/Web/typo3conf/ext/",
            "[ -L .Build/Web/typo3conf/ext/autourls ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/autourls"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}
